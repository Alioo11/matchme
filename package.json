{
  "name": "matchme",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": " node dist/app.js",
    "set-env-dev": "export NODE_ENV=development",
    "set-env-prod": "export NODE_ENV=production",
    "start:production": "npm run set-env-prod && npm run start",
    "start:development": "npm run set-env-dev && npm run start",
    "typescript:watch": "tsc -w ",
    "resolvePaths": "node -r tsconfig-paths/register dist/app.js",
    "nodemon:watch": "nodemon dist/app.js -w",
    "dev": "concurrently --kill-others \"npm run typescript:watch\" \"npm run nodemon:watch\"",
    "dev:production": "npm run set-env-prod && npm run dev",
    "dev:development": "npm run set-env-dev && npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/random-useragent": "^0.3.3",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "moment": "^2.30.1",
    "mongoose": "^6.3.4",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.7.1",
    "random-useragent": "^0.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-wiz": "^0.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
